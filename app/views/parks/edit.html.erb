<a href="/">Home</a>
<h1>Edit Park:</h1>

<%= form_for @park do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>

  <%= form.label :picture %>
  <%= form.text_field :picture %>

  <%= form.label :description %>
  <%= form.text_area :description %>

  <%= form.submit %>

<% end %>